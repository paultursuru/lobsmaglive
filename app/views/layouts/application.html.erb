    <title>Lobsmaglive</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all' %>
  </head>

  <body>
    <div class="header-lobs">
      <h1>L'OBSERVATOIRE</h1>
      <h5>MUSIQUE</h5>
    </div>
    <div class="other-pages">
      <%= link_to "MAGAZINE", "https://www.lobservatoire.co/magazine" %>
      <%= link_to "JEUNES CREATEURS", "https://www.lobservatoire.co/les-jeunes-createurs/" %>
      <%= link_to "STORE", "https://store-lobservatoire.co/" %>
